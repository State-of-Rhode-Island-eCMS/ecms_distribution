{
    "name": "rhodeislandeecms/ecms_distribution",
    "description": "The State of Rhode Island eCMS Drupal distribution.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Oomph, Inc.",
            "email": "devops@oomphinc.com",
            "homepage": "https://www.oomphinc.com/"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "git",
            "url": "git@github.com:State-of-Rhode-Island-eCMS/ecms_profile.git"
        },
        {
            "type": "git",
            "url": "https://github.com/State-of-Rhode-Island-eCMS/ecms_patternlab.git"
        },
        {
            "type": "package",
            "package": {
                "name": "drupal_git/migration_tools",
                "type": "drupal-module",
                "version": "1.0.0",
                "source": {
                    "type": "git",
                    "url": "https://git.drupalcode.org/project/migration_tools.git",
                    "reference": "3e193bc97d127ea2cff6b80f9509bc161bdee19f"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "drupal_git/migrate_process_trim",
                "type": "drupal-module",
                "version": "1.0.0",
                "source": {
                    "type": "git",
                    "url": "https://git.drupalcode.org/project/migrate_process_trim.git",
                    "reference": "79c7ceb9113c1e21818bd124135e5d261ccbebbc"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "nnnick/chartjs",
                "version": "v3.9.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/chartjs/Chart.js/releases/download/v3.9.1/chart.js-3.9.1.tgz",
                    "type": "tar"
                }
            }
        }
    ],
    "require": {
        "php": ">=8.1",
        "behat/mink": "^1.8",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/preserve-paths": "^0.1.4",
        "drupal/ckeditor": "^1.0",
        "drupal/config_update": "^2.0@alpha",
        "drupal/content_moderation_notifications": "^3.5",
        "drupal/core-composer-scaffold": "^10.1.5",
        "drupal/core-project-message": "^10.1.5",
        "drupal/core-recommended": "^10.1.5",
        "drupal/core-vendor-hardening": "^10.1.5",
        "drupal/features": "^3.13",
        "drupal/feeds": "^3.0@beta",
        "drupal/menu_force": "^2.0",
        "drupal/office_hours": "^1.11",
        "drush/drush": "^12.2.0",
        "mglaman/composer-drupal-lenient": "^1.0",
        "nnnick/chartjs": "^3.9",
        "npm-asset/jquery-ui-touch-punch": "^0.2.3",
        "oomphinc/composer-installers-extender": "^2.0",
        "rhodeislandecms/ecms_profile": "dev-RIGA-322/drupal-10-upgrade-part2-version2",
        "state-of-rhode-island-ecms/ecms_patternlab": "dev-RIGA-322/drupal-10-upgrade",
        "wikimedia/composer-merge-plugin": "^2.0.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "discard-changes": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "oomphinc/composer-installers-extender": true,
            "drupal-composer/preserve-paths": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "drupal/core-vendor-hardening": true,
            "wikimedia/composer-merge-plugin": true,
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "mglaman/composer-drupal-lenient": true
        },
        "process-timeout": 0
    },
    "scripts": {
        "post-install-cmd": [
            "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
            "patch -d docroot < patches/htaccess.patch"
        ],
        "post-update-cmd": [
            "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
            "patch -d docroot < patches/htaccess.patch"
        ],
        "test:php": [
            "vendor/bin/phpunit"
        ],
        "validate:php": [
            "vendor/bin/phpcs"
        ]
    },
    "extra": {
        "installer-types": [
            "bower-asset",
            "npm-asset",
            "pattern-lab"
        ],
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "docroot/profiles/contrib/ecms_profile/ecms_base/themes/custom/ecms/{$name}": [
                "type:pattern-lab"
            ]
        },
        "patches": {
            "drupal/core": {
                "2259567 - Unnecessary restrictions on logo format: Can't upload replacement SVG logo": "patches/drupal-core-2259567-allow-svg.patch",
                "3301239 - PoStreamReader::readLine() throws an error on module install": "https://www.drupal.org/files/issues/2023-01-17/drupal_core-PoStreamReader_readLine_throws_an_error_on_module_install-3301239-7.patch",
                "3359511 - [regression] missing menu active trail in Drupal 9.5.9": "patches/drupal-core-3359511-revert-3277784-2.patch"
            },
            "drupal/media_revisions_ui": {
                "3247661 - Add filename to output": "patches/3247661-media-revisions-filename.patch"
            },
            "drupal/webform_encrypt": {
                "3292305 - PHP Warning if unserialize fails": "https://www.drupal.org/files/issues/2022-06-23/unserialize-php-notice.patch",
                "2943344 - Add option to encrypt all elements on a webform": "https://www.drupal.org/files/issues/2021-05-28/webform_encrypt-encrypt_all-2943344-14.patch"
            },
            "drupal/easy_breadcrumb": {
                "Re-save string config values as arrays": "patches/easy_breadcrumb-install.patch",
                "3284123 - explode(): Passing null to parameter #2 ($string) of type string is deprecated": "https://www.drupal.org/files/issues/2022-09-20/fix_php81_explode-3284123-13.patch"
            },
            "drupal/feeds": {
                "3158678 - FetcherResult::checkFile() throws \\RuntimeException, which is never caught": "https://www.drupal.org/files/issues/2022-09-27/feeds-3158678-9-catch-runtime-errors.patch"
            },
            "drupal/paragraphs": {
                "3095959 - Paragraph type permissions conflicts with view unpublished paragraph permission": "https://www.drupal.org/files/issues/2019-11-22/paragraphs_type_permissions_view_unpublished_3095959-5.patch"
            },
            "drupal/migrate_process_trim": {
                "3288638 - Automated Drupal 10 compatibility fixes": "https://www.drupal.org/files/issues/2022-06-16/migrate_process_trim.2.0.x-dev.rector.patch"
            },
            "drupal/ckeditor_entity_link": {
                "3296754 - Automated Drupal 10 compatibility fixes": "https://www.drupal.org/files/issues/2022-08-22/ckeditor_entity_link.1.x-dev.rector.patch"
            }
        },
        "preserve-paths": [],
        "drupal-scaffold": {
            "locations": {
                "web-root": "docroot"
            },
            "file-mapping": {
                "[web-root]/robots.txt": false
            }
        },
        "enable-patching": "true",
        "merge-plugin": {
            "require": [
                "docroot/modules/contrib/webform/composer.libraries.json"
            ],
            "recurse": true,
            "replace": false,
            "ignore-duplicates": false,
            "merge-dev": true,
            "merge-extra": false,
            "merge-extra-deep": false,
            "merge-scripts": false
        },
        "drupal-lenient": {
            "allowed-list": ["drupal/ckeditor_entity_link"]
        }
    },
    "require-dev": {
        "drupal/coder": "^8.3",
        "drupal/core-dev": "^10.1.5",
        "drupal/devel": "^5.0",
        "phpunit/phpunit": "^9",
        "symfony/phpunit-bridge": "^6.3.2"
    }
}
